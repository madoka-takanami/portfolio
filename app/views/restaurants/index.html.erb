<%= content_for(:title, (t '.title')) %>
  <%= render 'shared/search_form', q: @my_list, url: restaurants_path %>
  <%= render 'random_select' %>
    <% if @restaurants.any? %>
      <%= render 'small_new' %>
      <div class="btm-space"><%= render @restaurants %></div>
    <% else %>
      <p class="text-center"><%= t('.not_have_lists') %></p>
      <%= render 'small_new' %>
    <% end %>

    <%= javascript_pack_tag 'random_select' %>
